# select-Kriterien für irgendwelche Aufnahmen (anpassen)
select marc_any_match('909[L2]a', 'LU|IS')

# Löschen und Korrigieren einiger Felder und Unterfelder
do marc_each()
  marc_remove(773$1$2$j$w)
  marc_remove(490$i$w)
  marc_remove(599)
  marc_remove(9..)
  marc_cut(***,tmp)
  rename(tmp.*.subfields.,1,0)
  marc_paste(tmp)
end
